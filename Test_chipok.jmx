            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">def startTime = Long.parseLong(vars.get(&quot;startTime&quot;));
def endTime = System.currentTimeMillis();
def executionTime = (endTime-startTime);
def delay =(1000 - executionTime).intValue();

log.info(&quot;Pause have to: ${delay/1000.0}&quot;)

if(delay &gt; 0) {
    vars.put(&quot;delay&quot;, String.valueOf(delay));
    log.info(String.valueOf(delay))
    return delay;
}else {
    vars.put(&quot;delay&quot;, String.valueOf(0));
    return 0;
}</stringProp>
